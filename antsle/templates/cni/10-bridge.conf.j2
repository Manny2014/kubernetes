{
    "cniVersion": "0.3.1",
    "name": "bridge",
    "type": "bridge",
    "bridge": "cnio0",
    "isGateway": true,
    "ipMasq": true,
    "ipam": {
        "type": "host-local",
        "ranges": [
          [{"subnet": "{{ pod_ip_cidr.split('.')[:2] | join('.') }}.{{ hostvars[inventory_hostname]['ansible_default_ipv4']['address'].split('.')[3] }}.0/24"}]
        ],
        "routes": [{"dst": "0.0.0.0/0"}]
    }
}