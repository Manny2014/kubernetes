apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: {{ kubernetes_conf_dir }}/ca.pem
    server: {{ master_url }}
  name: bootstrap
contexts:
- context:
    cluster: bootstrap
    user: kubelet-bootstrap
  name: bootstrap
current-context: bootstrap
preferences: {}
users:
- name: kubelet-bootstrap
  user:
    token: "{{ node_bootstrap_token_Id }}.{{ node_bootstrap_token}}"